! Rofi Settings
rofi.color-window: argb:EE2F343F, #2F343F
rofi.color-normal: argb:002F343F, #737783, argb:002F343F, argb:002F343F, #f9f9f9
rofi.color-urgent: argb:002F343F, #cc6666, argb:e54b5160, argb:002F343F, #f9f9f9
rofi.color-active: argb:002F343F, #65acff, argb:e44b5160, argb:002F343F, #f9f9f9
rofi.width: 20
rofi.lines: 16
rofi.font: Iosevka Nerd Font Light 16
rofi.bw: 0
rofi.padding: 10
rofi.eh: 1
rofi.sidebar-mode: true
rofi.yoffset: 31
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.show-icons: true
rofi.drun-icon-theme: Papirus-Dark
rofi.location: 1
rofi.modi: window,run,drun

! Urxvt settings
URxvt.font: xft:Iosevka Nerd Font:regular:size=15,xft:Iosevka Nerd Font Mono:regular:size=15:antialias=true:hinting:false
URxvt.scrollBar: false
URxvt.termName: rxvt-unicode
URxvt.letterSpace: 1
URxvt.saveLines: 10000
URxvt.scrollstyle: rxvt
URxvt.perl-ext-common: default,selection-to-clipboard,pasta,matcher,keyboard-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.underlineURLs: True
URxvt.matcher.button: 1

